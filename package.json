{
  "name": "webpack-browser-extension-plugin",
  "version": "1.1.0",
  "description": "A webpack extension that has auto reload, and some built in tools to help webpack browser extension development.",
  "main": "dist/index.js",
  "repository": "https://github.com/jcblw/webpack-browser-extension-plugin",
  "author": "Jacob Lowe <jacoblowe2.0@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublish": "yarn build",
    "test": "jest",
    "lint": "eslint -c ./.eslintrc.js ./src/**/*.ts",
    "watch": "tsc --watch",
    "start": "yarn watch",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/chrome": "^0.0.145",
    "@types/firefox-webext-browser": "^82.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/klaw": "^3.0.2",
    "@types/node": "^16.0.0",
    "@types/webpack": "4",
    "@types/ws": "^7.4.6",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "klaw": "^3.0.0",
    "webpack-inject-plugin": "^1.5.5",
    "ws": "^7.5.2"
  }
}
